<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ================================ Stack Array ====================== -->
	<SoundDef>
		<defName>AC_StackArray_Ambience_Start</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Electricity/StackArray/Start/StackArrayStart_1a</clipPath>
					</li>
				</grains>
				<volumeRange>18~18</volumeRange>
				<distRange>0~50.27907</distRange>
				<sustainLoop>False</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>
	<SoundDef>
		<defName>AC_StackArray_Ambience_Stop</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Electricity/StackArray/Stop/StackArrayStop_1a</clipPath>
					</li>
				</grains>
				<volumeRange>18~18</volumeRange>
				<distRange>0~50.27907</distRange>
				<sustainLoop>False</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>
	<SoundDef>
		<defName>AC_StackArray_Ambience</defName>
		<sustain>True</sustain>
		<context>MapOnly</context>
		<priorityMode>PrioritizeNearest</priorityMode>
		<sustainStartSound>AC_StackArray_Ambience_Start</sustainStartSound>
		<sustainStopSound>AC_StackArray_Ambience_Stop</sustainStopSound>
		<subSounds>
			<li>
				<muteWhenPaused>True</muteWhenPaused>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Electricity/StackArray/Run/StackArrayRun_Loop1a</clipPath>
					</li>
				</grains>
				<volumeRange>17~19</volumeRange>
				<distRange>0~30</distRange>
			</li>
		</subSounds>
	</SoundDef>
</Defs>