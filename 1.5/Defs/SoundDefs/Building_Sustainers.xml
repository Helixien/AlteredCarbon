		<defName>AC_SleeveGestator_Ambience</defName>
		<sustain>True</sustain>
		<context>MapOnly</context>
		<priorityMode>PrioritizeNearest</priorityMode>
		<sustainStartSound>AC_SleeveGestator_Ambience_Start</sustainStartSound>
		<sustainStopSound>AC_SleeveGestator_Ambience_Stop</sustainStopSound>
		<subSounds>
			<li>
				<muteWhenPaused>True</muteWhenPaused>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Electricity/SleeveVat/Run/SleeveVatRun_Loop1a</clipPath>
