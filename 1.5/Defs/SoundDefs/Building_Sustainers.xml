<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ================================ Sleeve Gestator ====================== -->
	<SoundDef>
		<defName>AC_SleeveGestator_Ambience_Start</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Electricity/SleeveVat/Start/SleeveVatStart_1a</clipPath>
					</li>
				</grains>
				<volumeRange>16~16</volumeRange>
				<distRange>0~50.27907</distRange>
				<sustainLoop>False</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>
	<SoundDef>
		<defName>AC_SleeveGestator_Ambience_Stop</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Electricity/SleeveVat/Stop/SleeveVatStop_1a</clipPath>
