<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ================================ Duplicate Stack ====================== -->
	<JobDef>
		<defName>AC_DuplicateStack</defName>
		<driverClass>AlteredCarbon.JobDriver_DuplicateStack</driverClass>
		<reportString>duplicating persona stack</reportString>
	</JobDef>
	<!-- ================================ Create Stack from PersonaPrint ====================== -->
	<JobDef>
		<defName>AC_CreateStackFromPersonaPrint</defName>
		<driverClass>AlteredCarbon.JobDriver_CreateStackFromPersonaPrint</driverClass>
		<reportString>restoring persona stack from persona print</reportString>
	</JobDef>
	<!-- ================================ Hauling Persona Prints ====================== -->
	<JobDef>
		<defName>AC_HaulPersonaPrintsToPersonaMatrix</defName>
		<driverClass>AlteredCarbon.JobDriver_HaulPersonaPrintsToPersonaMatrix</driverClass>
		<reportString>hauling TargetA to TargetB.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>
</Defs>