<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ================================ Sleeve Gestator Addons - Base ====================== -->
	<ThingDef ParentName="BuildingBase" Name="AC_SleeveGestatorLinkableBase" Abstract="True">
		<drawerType>MapMeshAndRealTime</drawerType>
		<passability>PassThroughOnly</passability>
		<pathCost>25</pathCost>
		<blockWind>false</blockWind>
		<fillPercent>0.4</fillPercent>
		<canOverlapZones>false</canOverlapZones>
		<rotatable>false</rotatable>
		<designationCategory>Biotech</designationCategory>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<constructionSkillPrerequisite>10</constructionSkillPrerequisite>
		<researchPrerequisites>
			<li>AC_AdvancedSleeving</li>
		</researchPrerequisites>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<comps>
			<li Class="CompProperties_Flickable"/>
		</comps>
	</ThingDef>
	<!-- ================================ Sleeve Gestator Addons - Neutroamine Injector Pump ====================== -->
	<ThingDef ParentName="AC_SleeveGestatorLinkableBase">
		<defName>AC_NeutroaminePump</defName>
		<label>neutroamine injector pump</label>
		<description>text.todo</description>
