<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ================================ Sleeve Gestator Addons - Base ====================== -->
	<ThingDef ParentName="BuildingBase" Name="AC_SleeveGestatorLinkableBase" Abstract="True">
		<drawerType>MapMeshAndRealTime</drawerType>
		<passability>PassThroughOnly</passability>
		<pathCost>25</pathCost>
		<blockWind>false</blockWind>
		<fillPercent>0.4</fillPercent>
		<canOverlapZones>false</canOverlapZones>
		<rotatable>false</rotatable>
		<designationCategory>Biotech</designationCategory>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<constructionSkillPrerequisite>10</constructionSkillPrerequisite>
		<researchPrerequisites>
			<li>AC_AdvancedSleeving</li>
		</researchPrerequisites>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<comps>
			<li Class="CompProperties_Flickable"/>
		</comps>
	</ThingDef>
	<!-- ================================ Sleeve Gestator Addons - Neutroamine Injector Pump ====================== -->
	<ThingDef ParentName="AC_SleeveGestatorLinkableBase">
		<defName>AC_NeutroaminePump</defName>
		<label>neutroamine injector pump</label>
		<description>text.todo</description>
		<graphicData>
			<texPath>Things/Building/Misc/NeutroamineInjectorPump</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shadowData>
				<volume>(0.9,0.2,0.9)</volume>
			</shadowData>
			<drawSize>(1, 1)</drawSize>
		</graphicData>
		<size>(1,1)</size>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Mass>10</Mass>
		</statBases>
		<building>
			<destroySound>BuildingDestroyed_Metal_Small</destroySound>
		</building>
		<costList>
			<Plasteel>25</Plasteel>
			<ComponentIndustrial>2</ComponentIndustrial>
			<ComponentSpacer>2</ComponentSpacer>
		</costList>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>75</basePowerConsumption>
			</li>
			<li Class="CompProperties_Facility">
				<maxSimultaneous>4</maxSimultaneous>
				<maxDistance>12</maxDistance>
			</li>
		</comps>
	</ThingDef>
