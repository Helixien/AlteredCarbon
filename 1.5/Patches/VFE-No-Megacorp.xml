<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Megacorp</li>
		</mods>
		<nomatch Class="PatchOperationSequence">
			<operations>
				<!-- ================================ Changes to the Ultratech buildings ====================== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AC_SleeveIncubator"]</xpath>
					<value>
						<researchPrerequisites>
							<li>AC_Sleeving</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AC_SleeveCasket"]</xpath>
					<value>
						<researchPrerequisites>
							<li>AC_Sleeving</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AC_DecryptionBench"]</xpath>
					<value>
						<researchPrerequisites>
							<li>AC_Hacking</li>
						</researchPrerequisites>
					</value>
				</li>
			</operations>
		</nomatch>
	</Operation>
</Patch>